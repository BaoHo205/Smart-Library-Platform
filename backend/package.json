    "@typescript-eslint/parser": "^8.38.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
